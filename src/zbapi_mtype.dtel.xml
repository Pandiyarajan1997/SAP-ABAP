     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>ZBAPI_MTYPE</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00007</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Der Parameter kann folgende Werte haben:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>&apos;S&apos; für Erfolgsmeldungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>&apos;E&apos; für Fehlersituationen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>&apos;W&apos; für Warnungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>&apos;I&apos; für Informationsmeldungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>&apos;A&apos; für Abbruchmeldungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Abhängig von der Implementierung eines BAPIs kann bei Abbruchmeldungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>ein Rollback auf der Datenbank ausgeführt werden. Dies bedeutet, daß</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>alle Datenbankänderungen seit dem letzten Commit Work-Kommando</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zurückgenommen werden. Informationen hierzu finden Sie in der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Dokumentation zu dem betreffenden BAPI.</TDLINE>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>DE</ID>
