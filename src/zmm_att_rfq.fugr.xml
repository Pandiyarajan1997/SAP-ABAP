   <FUNCTIONS>
    <item>
     <FUNCNAME>ZMM_RFQ_ATT_MAIL</FUNCNAME>
     <SHORT_TEXT>ME41 - RFQ mail to vendor address as PDF</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>I_EKKO</PARAMETER>
       <TYP>EKKO</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>I_MAIL_ID</PARAMETER>
       <TYP>AD_SMTPADR</TYP>
      </RSIMP>
     </IMPORT>
     <TABLES>
      <RSTBL>
       <PARAMETER>IM_EKPO</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <TYP>MMPR_UEKPO</TYP>
      </RSTBL>
     </TABLES>
     <EXCEPTION>
      <RSEXC>
       <EXCEPTION>NO_MAIL_ID_FOUND</EXCEPTION>
      </RSEXC>
     </EXCEPTION>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>I_EKKO</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Purchasing Document Number</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>I_MAIL_ID</PARAMETER>
       <KIND>P</KIND>
       <STEXT>E-Mail Address</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_EKPO</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Change Document Structure; Generated by RSSCD000</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>NO_MAIL_ID_FOUND</PARAMETER>
       <KIND>X</KIND>
       <STEXT>No Vendor Mail id found</STEXT>
      </RSFDO>
     </DOCUMENTATION>
    </item>
