   <cls:CL_FP_CODING id="o5">
    <CL_FP_INTERFACE_DATA_CHANGED classVersion="1">
     <INTERFACE_DATA href="#o1"/>
    </CL_FP_INTERFACE_DATA_CHANGED>
    <CL_FP_CODING classVersion="1">
     <INPUT_PARAMETERS/>
     <OUTPUT_PARAMETERS/>
     <INITIALIZATION>
      <FPCLINE>SELECT SINGLE ADRNR FROM kna1</FPCLINE>
      <FPCLINE>                    INTO lv_addr</FPCLINE>
      <FPCLINE>                    WHERE kunnr = DISTRIBUTOR.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>*BREAK cnabap.</FPCLINE>
      <FPCLINE>**********************************************************************Distributor Address</FPCLINE>
      <FPCLINE>select single from kna1</FPCLINE>
      <FPCLINE>fields adrnr</FPCLINE>
      <FPCLINE>WHERE kunnr = @DISTRIBUTOR</FPCLINE>
      <FPCLINE>INTO @data(dist_adrnr).</FPCLINE>
      <FPCLINE/>
      <FPCLINE>Select SINGLE from adrc</FPCLINE>
      <FPCLINE>fields name1,street,city1,tel_number,post_code1</FPCLINE>
      <FPCLINE>WHERE addrnumber = @dist_adrnr</FPCLINE>
      <FPCLINE>INTO @lv_dist.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>lv_dist-name = |{ lv_dist-name } ({ DISTRIBUTOR })|.</FPCLINE>
      <FPCLINE>*lv_dist-city = |{ lv_dist-street } { lv_dist-city }|.</FPCLINE>
      <FPCLINE>**********************************************************************Customer Address</FPCLINE>
      <FPCLINE>data(lv_Cust) = gt_kna1[ 1 ].</FPCLINE>
      <FPCLINE/>
      <FPCLINE>Select SINGLE from adrc</FPCLINE>
      <FPCLINE>fields name1,street,city1,tel_number,post_code1</FPCLINE>
      <FPCLINE>WHERE addrnumber = @lv_Cust-adrnr</FPCLINE>
      <FPCLINE>INTO @lv_dealr.</FPCLINE>
      <FPCLINE>*</FPCLINE>
      <FPCLINE>lv_dealr-name = |{ lv_dealr-name } ({ lv_Cust-kunnr })|.</FPCLINE>
      <FPCLINE>*lv_dealr-city = |{ lv_dealr-street } { lv_dealr-city }|.</FPCLINE>
      <FPCLINE>data: lv_bal type ty_bal.</FPCLINE>
      <FPCLINE>clear lv_bal.</FPCLINE>
      <FPCLINE/>
      <FPCLINE>LOOP at GT_BSID into gs_final.</FPCLINE>
      <FPCLINE>  if sy-tabix = 1.</FPCLINE>
      <FPCLINE> if gs_final-cr_amt is NOT INITIAL.</FPCLINE>
      <FPCLINE>    lv_bal-Bal_amt = lv_opn * -1.</FPCLINE>
      <FPCLINE>    else.</FPCLINE>
      <FPCLINE>      lv_bal-Bal_amt = lv_opn .</FPCLINE>
      <FPCLINE> endif.</FPCLINE>
      <FPCLINE>  ELSEIF sy-tabix = 2.</FPCLINE>
      <FPCLINE>  lv_bal-Bal_amt = lv_bal-Bal_amt  + gs_final-dr_amt - gs_final-cr_amt.</FPCLINE>
      <FPCLINE>  ELSEIF sy-tabix &gt; 2.</FPCLINE>
      <FPCLINE>   lv_bal-Bal_amt = lv_bal-Bal_amt + gs_final-dr_amt - gs_final-cr_amt.</FPCLINE>
      <FPCLINE>    endif.</FPCLINE>
      <FPCLINE>  APPEND lv_bal to gt_bal.</FPCLINE>
      <FPCLINE>  ENDLOOP.</FPCLINE>
     </INITIALIZATION>
     <FORMS/>
    </CL_FP_CODING>
   </cls:CL_FP_CODING>
